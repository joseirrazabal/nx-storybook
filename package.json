  },
  "peerDependencies": {
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "react-i18next": "^11.7.2"
  },
  "greenkeeper": {
    "ignore": [
      "webpack"
    ]
  }
}
