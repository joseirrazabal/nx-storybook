{
  "name": "@joseirrazabal/nxbook",
  "version": "0.0.9",
  "description": "Libreria de componentes del proyecto",
  "scripts": {
    "dist": "npm run build && cp package.json .npmrc lib && cd lib && npm publish",
    "prebuild": "npm run clean",
    "clean": "rimraf esm",
    "dev": "nodemon --ignore lib/ --ignore dev/ -e js,jsx --exec npm run build:cjs",
    "build": "npm run build:cjs",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --presets=@babel/env,@babel/react --plugins @babel/plugin-syntax-dynamic-import,@babel/plugin-syntax-import-meta,@babel/plugin-proposal-class-properties,@babel/plugin-proposal-json-strings src/ -d lib/",
    "start": "start-storybook -p 6006",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^react-dom(.*)$": "<rootDir>/node_modules/react-dom2$1",
      "^react/(.*)$": "<rootDir>/node_modules/react2$1",
      "^@material-ui/core(.*)$": "<rootDir>/node_modules/@material-ui2/core$1",
      "^Components(.*)$": "<rootDir>/src$1"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-react-jsx-source": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-options": "^5.2.8",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^6.0.21",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-preset-react-app": "^9.1.2",
    "cache-loader": "^4.1.0",
    "clean-css": "^4.2.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-react-with-styles": "^2.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.0.2",
    "git-directory-deploy": "^1.5.1",
    "husky": "^3.1.0",
    "imports-loader": "^0.8.0",
    "jest": "^26.4.2",
    "json-loader": "^0.5.7",
    "moment": "^2.20.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react-i18next": "^11.7.2",
    "react-spinners": "^0.9.0",
    "react-virtualized": "^9.22.2"
  },
  "peerDependencies": {
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1"
  },
  "greenkeeper": {
    "ignore": [
      "webpack"
    ]
  }
}
